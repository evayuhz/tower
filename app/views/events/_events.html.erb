<% @events_group.each do |day, day_events| %>
<% if day_events.any? %>
  <div class="events-day  <%= (day.saturday? || day.sunday?) ? 'weekend' : '' %> ">
    <h4 class="events-day-title"><%= event_day_format(day) %></h4>
    <div class="events-day-content">
    <% events_groups = group_event_by_project_and_time(day_events) %>
    <% events_groups.each do |events_group| %>
      <div class="events-ancestor">
        <h5 class="events-ancestor-title">
          <%= link_to events_group[0].name, project_path(events_group[0]) %>
        </h5>
        <%= render events_group[1] %>
      </div>
    <% end %>
    </div>
  </div>
<% end %>
<% end %>
